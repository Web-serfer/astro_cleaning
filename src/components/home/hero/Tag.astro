---
interface Props {
  text: string
}
const { text } = Astro.props
---

<div class="flex items-center gap-2">
    <span class="inline-flex items-center gap-2 border border-white rounded-full py-1 px-3 text-xs text-white font-medium">
        <span class="w-3 h-3 bg-white rounded-full" aria-hidden="true"></span>
        {text}
    </span>
</div>